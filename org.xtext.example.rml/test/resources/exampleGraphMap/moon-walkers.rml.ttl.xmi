<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="crm:" iri="http://www.cidoc-crm.org/cidoc-crm/"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<directives prefix="ex:" iri="http://example.com/"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<triplesMaps name="MoonWalkersMapping">
		<logicalSource >
			<source path="src/test/resources/example7/moon-walkers7.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/{Name}"/>
			<class>crm:E21_Person</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="MoonWalkersMapping">
			<predicateMaps>
				<constant value="ex:birthday"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="MoonWalkersMapping">
				<reference value="Birth Date"/>
				<datatype value="xsd:date"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="MoonWalkersMapping">
			<predicateMaps>
				<constant value="ex:deathday"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="MoonWalkersMapping">
				<reference value="Death Date"/>
				<datatype value="xsd:date"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="MoonWalkersMapping">
			<predicateMaps>
				<constant value="ex:evaday"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="MoonWalkersMapping">
				<reference value="EVA Date"/>
				<datatype value="xsd:date"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="MoonWalkersMapping">
			<predicateMaps>
				<constant value="ex:service"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="MoonWalkersMapping">
				<reference value="Service"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
