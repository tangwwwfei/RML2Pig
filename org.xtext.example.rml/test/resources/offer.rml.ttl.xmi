<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="schema:" iri="http://schema.org/"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="crm:" iri="http://www.cidoc-crm.org/cidoc-crm/"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<triplesMaps name="offer">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="events-listing.csv"/>
		</logicalSource>
		<subjectMap>
			<termtype value="rr:BlankNode"/>
			<class>schema:Offer</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="offer">
			<predicateMaps>
				<constant value="schema:url"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="offer">
				<reference value="ticket_url"/>
				<termtype value="rr:IRI"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
