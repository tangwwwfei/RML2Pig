<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="crm:" iri="http://www.cidoc-crm.org/cidoc-crm/"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<directives prefix="ex:" iri="http://example.com/"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<triplesMaps name="ArtistMapping">
		<logicalSource  iterator="$.[*]" referenceFormulation="ql:JSONPath">
			<source path="src/test/resources/example5/museum.json"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/{Artist}"/>
			<class>crm:E21_Person</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="ArtistMapping">
			<predicateMaps>
				<constant value="rdfs:label"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="ArtistMapping">
				<reference value="Artist"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="ArtistMapping">
			<predicateMaps>
				<constant value="crm:P143_carried_out_by"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="ArtistMapping" parentTriplesMap="MoonWalkersMapping">
				<joinConditions ownTriplesMap="ArtistMapping" parent="Name" child="Artist">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="MoonWalkersMapping">
		<logicalSource  iterator="$.[*].Sitter[*]" referenceFormulation="ql:JSONPath">
			<source path="src/test/resources/example5/museum.json"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/{Name}"/>
			<class>crm:E21_Person</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="MoonWalkersMapping">
			<predicateMaps>
				<constant value="rdfs:label"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="MoonWalkersMapping">
				<reference value="$Name"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="SitterMapping">
		<logicalSource  iterator="$.[*].Sitter" referenceFormulation="ql:JSONPath">
			<source path="src/test/resources/example5/museum.json"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/{Name}"/>
			<class>crm:E21_Person</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="SitterMapping">
			<predicateMaps>
				<constant value="rdfs:label"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SitterMapping">
				<reference value="Name"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="ProductionMapping">
		<logicalSource  iterator="$.[*]" referenceFormulation="ql:JSONPath">
			<source path="src/test/resources/example5/museum.json"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/Production/{Ref}"/>
			<class>crm:E12_Production</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="ProductionMapping">
			<predicateMaps>
				<constant value="crm:P82_at_some_time_within"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="ProductionMapping">
				<reference value="DateOfWork"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="ProductionMapping">
			<predicateMaps>
				<constant value="crm:P14_carried_out_by"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="ProductionMapping" parentTriplesMap="ArtistMapping">
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="ProductionMapping">
			<predicateMaps>
				<constant value="crm:P108_has_produced"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="ProductionMapping" parentTriplesMap="ArtworkMapping">
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="ArtworkMapping">
		<logicalSource  iterator="$.[*]" referenceFormulation="ql:JSONPath">
			<source path="src/test/resources/example5/museum.json"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/{Ref}"/>
			<class>crm:E22_Man-Made_Object</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="ArtworkMapping">
			<predicateMaps>
				<constant value="crm:P102_has_title"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="ArtworkMapping">
				<reference value="Title"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="ArtworkMapping">
			<predicateMaps>
				<constant value="crm:P48_has_preferred_identifier"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="ArtworkMapping">
				<reference value="Ref"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="ArtworkMapping">
			<predicateMaps>
				<constant value="crm:P62_depicts"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="ArtworkMapping" parentTriplesMap="SitterMapping">
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="WikipediaExportMapping">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="src/test/resources/example5/moon-walkers.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/{Name}"/>
			<class>crm:E21_Person</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="WikipediaExportMapping">
			<predicateMaps>
				<constant value="ex:birthday"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="WikipediaExportMapping">
				<reference value="Birth Date"/>
				<datatype value="xsd:date"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="WikipediaExportMapping">
			<predicateMaps>
				<constant value="ex:deathday"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="WikipediaExportMapping">
				<reference value="Death Date"/>
				<datatype value="xsd:date"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="WikipediaExportMapping">
			<predicateMaps>
				<constant value="ex:evaday"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="WikipediaExportMapping">
				<reference value="EVA Date"/>
				<datatype value="xsd:date"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="WikipediaExportMapping">
			<predicateMaps>
				<constant value="ex:service"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="WikipediaExportMapping">
				<reference value="Service"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
