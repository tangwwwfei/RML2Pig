<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="im:" iri="http://semweb.mmlab.be/ns/im#"/>
	<directives prefix="cerif:" iri="http://spi-fm.uca.es/neologism/cerif#"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="foaf:" iri="http://xmlns.com/foaf/0.1/"/>
	<directives prefix="dcterms:" iri="http://purl.org/dc/terms/"/>
	<directives prefix="bibo:" iri="http://purl.org/ontology/bibo/"/>
	<directives prefix="skos:" iri="http://www.w3.org/2004/02/skos/core#"/>
	<triplesMaps name="AuthorMapping">
		<logicalSource  uriRef="AuthorsInput">
		</logicalSource>
		<subjectMap>
			<template value="http://explore.ilastic.be/person/{CRM ID}"/>
			<class>cerif:Person</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="AuthorMapping">
			<predicateMaps>
				<constant value="cerif:internalidentifier"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AuthorMapping">
				<reference value="Auteur iMinds personnelID"/>
				<datatype value="xsd:positiveInteger"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AuthorMapping">
			<predicateMaps>
				<constant value="dcterms:identifier"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AuthorMapping">
				<reference value="CRM ID"/>
				<languageTag>en-us</languageTag>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AuthorMapping">
			<predicateMaps>
				<constant value="cerif:name"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AuthorMapping">
				<template value="{Auteur voornaam} {Auteur naam}"/>
				<termtype value="rr:Literal"/>
				<languageTag>en-us</languageTag>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AuthorMapping">
			<predicateMaps>
				<constant value="foaf:givenName"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AuthorMapping">
				<reference value="Auteur voornaam"/>
				<languageTag>en-us</languageTag>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AuthorMapping">
			<predicateMaps>
				<constant value="foaf:familyName"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AuthorMapping">
				<reference value="Auteur naam"/>
				<languageTag>en-us</languageTag>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AuthorMapping">
			<predicateMaps>
				<constant value="cerif:linksToOrganisationUnit"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AuthorMapping" parentTriplesMap="GroupMapping">
				<joinConditions ownTriplesMap="AuthorMapping" parent="Group v/d auteur - CRM ID" child="Groep v/d auteur - CRM ID">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AuthorMapping">
			<predicateMaps>
				<constant value="cerif:linksToOrganisationUnit"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AuthorMapping" parentTriplesMap="UniversityMapping">
				<joinConditions ownTriplesMap="AuthorMapping" parent="Groep v/d auteur - kenniscentrum code" child="Auteur kenniscentrum">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AuthorMapping">
			<predicateMaps>
				<constant value="cerif:linksToPublication"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AuthorMapping" parentTriplesMap="PublicationMapping">
				<joinConditions ownTriplesMap="AuthorMapping" parent="publicatie (ID)" child="publicatie (ID)">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="GroupMapping">
		<logicalSource  uriRef="GroupInput">
		</logicalSource>
		<subjectMap>
			<template value="http://explore.ilastic.be/organization/{Groep v/d auteur - CRM ID}"/>
			<class>cerif:OrganizationalUnit</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="GroupMapping">
			<predicateMaps>
				<constant value="cerif:internalidentifier"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="GroupMapping">
				<reference value="Groep v/d auteur"/>
				<languageTag>en-us</languageTag>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="GroupMapping">
			<predicateMaps>
				<constant value="cerif:acronym"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="GroupMapping">
				<reference value="Groep v/d auteur"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="GroupMapping">
			<predicateMaps>
				<constant value="dcterms:identifier"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="GroupMapping">
				<reference value="Groep v/d auteur - CRM ID"/>
				<languageTag>en-us</languageTag>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="UniversityMapping">
		<logicalSource  uriRef="GroupInput">
		</logicalSource>
		<subjectMap>
			<template value="http://explore.ilastic.be/university/{Groep v/d auteur - kenniscentrum code}"/>
			<class>cerif:OrganizationalUnit</class>
		</subjectMap>
	</triplesMaps>
	<triplesMaps name="PublicationMapping">
		<logicalSource  uriRef="PublicationsInput">
		</logicalSource>
		<subjectMap>
			<template value="http://explore.ilastic.be/publication/{publicatie (ID)}"/>
			<class>cerif:Publication,bibo:Document</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="cerif:internalidentifier"/>
			</predicateMaps>
			<predicateMaps>
				<constant value="bibo:identifier"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="publicatie (ID)"/>
				<datatype value="xsd:positiveInteger"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="cerif:title"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="Titel"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="cerif:abstract"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="Abstract"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="bibo:issn"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="ISSN"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="bibo:isbn13"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="ISBN"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="bibo:uri"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="URL"/>
				<termtype value="rr:IRI"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="im:webOfScience"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="WOS"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="dcterms:issued"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="Jaar van publicatie"/>
				<datatype value="xsd:gYear"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="im:publishedIn"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="Gepubliceerd in"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="im:publishedIn"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="Conferentie titel"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="im:publicationCategory"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping">
				<reference value="Publication category"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="cerif:linksToOrganisationUnit"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping" parentTriplesMap="GroupMapping">
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="PublicationMapping">
			<predicateMaps>
				<constant value="cerif:linksToOrganisationUnit"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PublicationMapping" parentTriplesMap="UniversityMapping">
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
