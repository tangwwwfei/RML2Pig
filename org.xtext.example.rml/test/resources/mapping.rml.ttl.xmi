<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="foaf:" iri="http://xmlns.com/foaf/0.1/"/>
	<directives prefix="dcterms:" iri="http://purl.org/dc/terms/"/>
	<directives prefix="dome:" iri="http://domeontology.it/"/>
	<triplesMaps name="Person">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="http://rml.io/data/drafts/comma_seperated.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://somenamespace.it/Person/{first_name}{last_name}"/>
			<class>foaf:Person</class>
			<graphMaps>
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="Person">
			<predicateMaps>
				<constant value="foaf:first_name"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Person">
				<reference value="first_name"/>
			</objectMaps>
			<graphMaps ownTriplesMap="Person">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Person">
			<predicateMaps>
				<constant value="foaf:last_name"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Person">
				<reference value="last_name"/>
			</objectMaps>
			<graphMaps ownTriplesMap="Person">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Person">
			<predicateMaps>
				<constant value="foaf:name"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Person">
				<template value="{first_name} {last_name}"/>
				<termtype value="rr:Literal"/>
			</objectMaps>
			<graphMaps ownTriplesMap="Person">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Person">
			<predicateMaps>
				<constant value="foaf:birthday"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Person">
				<reference value="birthdate"/>
				<datatype value="xsd:date"/>
			</objectMaps>
			<graphMaps ownTriplesMap="Person">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Person">
			<predicateMaps>
				<constant value="foaf:member"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Person" parentTriplesMap="Company">
			</objectMaps>
			<graphMaps ownTriplesMap="Person">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Person">
			<predicateMaps>
				<constant value="dome:family_id"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Person">
				<reference value="family_id"/>
				<datatype value="xsd:integer"/>
			</objectMaps>
			<graphMaps ownTriplesMap="Person">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Person">
			<predicateMaps>
				<constant value="foaf:member"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Person" parentTriplesMap="FamilyType">
				<joinConditions ownTriplesMap="Person" parent="family_type" child="family_type">
				</joinConditions>
			</objectMaps>
			<graphMaps ownTriplesMap="Person">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="Company">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="http://rml.io/data/drafts/comma_seperated.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://somenamespace.it/Company/{company}"/>
			<class>foaf:Person</class>
			<graphMaps>
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="Company">
			<predicateMaps>
				<constant value="foaf:name"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Company">
				<reference value="company"/>
			</objectMaps>
			<graphMaps ownTriplesMap="Company">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="FamilyType">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="http://rml.io/data/drafts/family_type.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://somenamespace.it/FamilyType/{family_type}/{description}"/>
			<graphMaps>
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="FamilyType">
			<predicateMaps>
				<constant value="rdfs:label"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="FamilyType">
				<reference value="description"/>
			</objectMaps>
			<graphMaps ownTriplesMap="FamilyType">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="FamilyType">
			<predicateMaps>
				<constant value="dcterms:subject"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="FamilyType">
				<reference value="family_type"/>
			</objectMaps>
			<graphMaps ownTriplesMap="FamilyType">
				<constant value="http://somegraphname.it"/>
			</graphMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
