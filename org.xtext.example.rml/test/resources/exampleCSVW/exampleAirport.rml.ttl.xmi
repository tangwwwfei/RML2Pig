<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="csvw:" iri="http://www.w3.org/ns/csvw#"/>
	<directives prefix="mail:" iri="http://example.com/mail#"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="ex:" iri="http://www.example.com/"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<directives prefix="rdf:" iri="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
	<directives prefix="transit:" iri="http://vocab.org/transit/terms/"/>
	<directives prefix="wgs84_pos:" iri="http://www.w3.org/2003/01/geo/wgs84_pos#"/>
	<triplesMaps name="AirportMapping">
		<logicalSource  referenceFormulation="ql:CSV">
			<source>
				<csvonWeb url="http://rml.io/data/csvw/Airport.csv">
					<dialect trim="%221%22^^xsd:boolean" headerRowCount="%221%22^^xsd:nonNegativeInteger" header="%221%22^^xsd:boolean" encoding="UTF-8" delimiter=";">
					</dialect>
				</csvonWeb>
			</source>
		</logicalSource>
		<subjectMap>
			<template value="http://airport.example.com/{id}"/>
			<class>transit:Stop</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="AirportMapping">
			<predicateMaps>
				<constant value="transit:service"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AirportMapping">
				<reference value="bus"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AirportMapping">
			<predicateMaps>
				<constant value="wgs84_pos:location"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AirportMapping">
				<reference value="city"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
