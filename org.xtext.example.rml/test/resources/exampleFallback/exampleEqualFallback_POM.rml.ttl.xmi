<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="crml:" iri="http://semweb.mmlab.be/ns/crml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="dcat:" iri="http://www.w3.org/ns/dcat#"/>
	<directives prefix="sd:" iri="http://www.w3.org/ns/sparql-service-description#"/>
	<directives prefix="dcterms:" iri="http://purl.org/dc/terms/"/>
	<directives prefix="ex:" iri="http://example.com/"/>
	<directives base="http://example.com/base"/>
	<triplesMaps name="Companies">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="src/test/resources/exampleFallback/Companies.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/comp/{id}"/>
			<class>ex:Company</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="Companies">
			<predicateMaps>
				<constant value="ex:country"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Companies" parentTriplesMap="Country_Local_DBpedia">
			</objectMaps>
			<equalConditions ownTriplesMap="Companies">
				<condition>!match(%%country%%,\%22NewZealand\%22)</condition>
				<binding ownTriplesMap="Companies">
					<reference value="country"/>
					<variable>country</variable>
				</binding>
			</equalConditions>
			<fallbackMaps ownTriplesMap="Companies">
				<predicateMaps>
					<constant value="ex:country2"/>
				</predicateMaps>
				<objectMaps ownTriplesMap="Companies" parentTriplesMap="Country_Local">
				</objectMaps>
			</fallbackMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="Country_Local">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="src/test/resources/exampleFallback/Companies.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://ex.com/country/{country}"/>
		</subjectMap>
	</triplesMaps>
	<triplesMaps name="Country_Local_DBpedia">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="src/test/resources/exampleFallback/Companies.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://dbpedia.org/resource/{country}"/>
		</subjectMap>
	</triplesMaps>
</rml:Mapping>
