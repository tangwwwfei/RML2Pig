<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ex:" iri="http://example.com/ns#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="simmodel:" iri="http://www.lbl.gov/namespaces/Sim/Model/"/>
	<directives prefix="simres:" iri="http://www.lbl.gov/namespaces/Sim/ResourcesGeneral/"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<triplesMaps name="SimModelMapping">
		<logicalSource  iterator="/SimModel/simmodel:SimAppLibraryDefault_AppLibraryDefault_GlobalLibrary" referenceFormulation="ql:XPath">
			<source path="src/test/resources/example8/simergy.xml"/>
		</logicalSource>
		<subjectMap>
			<template value="http://www.lbl.gov/namespaces/Sim/Model/{@RefId}"/>
			<class>simmodel:SimAppLibraryDefault_AppLibraryDefault_GlobalLibrary</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="SimModelMapping">
			<predicateMaps>
				<constant value="simmodel:refId"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SimModelMapping">
				<reference value="@RefId"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="SimModelMapping">
			<predicateMaps>
				<constant value="simres:simModelType"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SimModelMapping">
				<reference value="simres:SimModelType"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="SimModelMapping">
			<predicateMaps>
				<constant value="simres:SimModelSubtype"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SimModelMapping">
				<reference value="simres:SimModelSubtype"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="SimModelMapping">
			<predicateMaps>
				<constant value="simres:SimModelName"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SimModelMapping">
				<reference value="simres:SimModelName"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="SimModelMapping">
			<predicateMaps>
				<constant value="simres:SourceModelSchema"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SimModelMapping">
				<reference value="simres:SourceModelSchema"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="SimModelMapping">
			<predicateMaps>
				<constant value="simres:DefaultPreferenceIndex"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SimModelMapping">
				<reference value="simres:DefaultPreferenceIndex"/>
				<datatype value="xsd:int"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="SimModelMapping">
			<predicateMaps>
				<constant value="simres:LibraryLocation"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SimModelMapping">
				<reference value="simres:LibraryLocation"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="SimModelMapping">
			<predicateMaps>
				<constant value="simres:LibraryName"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="SimModelMapping">
				<reference value="simres:LibraryName"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
