<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="foaf:" iri="http://xmlns.com/foaf/0.1/"/>
	<directives prefix="ex:" iri="http://example.com/"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="fnml:" iri="http://semweb.mmlab.be/ns/fnml#"/>
	<directives prefix="fno:" iri="http://w3id.org/function/ontology#"/>
	<directives prefix="grel:" iri="http://users.ugent.be/~bjdmeest/function/grel.ttl#"/>
	<directives base="http://example.com/base/"/>
	<triplesMaps name="TriplesMap1">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="../student.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://example.com/{Name}"/>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="TriplesMap1">
			<predicateMaps>
				<constant value="foaf:name"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="TriplesMap1">
				<functionMaps ownTriplesMap="TriplesMap1">
					<predicateObjectMaps ownTriplesMap="TriplesMap1">
						<predicateMaps>
							<constant value="fno:executes"/>
						</predicateMaps>
						<objectMaps ownTriplesMap="TriplesMap1">
							<template value="http://users.ugent.be/~bjdmeest/function/grel.ttl#{Class}"/>
						</objectMaps>
					</predicateObjectMaps><predicateObjectMaps ownTriplesMap="TriplesMap1">
						<predicateMaps>
							<constant value="grel:valueParameter"/>
						</predicateMaps>
						<objectMaps ownTriplesMap="TriplesMap1">
							<reference value="Name"/>
						</objectMaps>
					</predicateObjectMaps>
				</functionMaps>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
