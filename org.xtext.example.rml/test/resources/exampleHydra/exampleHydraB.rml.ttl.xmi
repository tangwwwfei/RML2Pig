<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="crml:" iri="http://semweb.mmlab.be/ns/rml/condition#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="mail:" iri="http://example.com/mail#"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="ex:" iri="http://www.example.com/"/>
	<directives prefix="hydra:" iri="http://www.w3.org/ns/hydra/core#"/>
	<directives base="http://example.com/base"/>
	<triplesMaps name="PaperAuthor">
		<logicalSource  iterator="$.author[*]" referenceFormulation="ql:JSONPath">
			<source>
				<webAPI template="https://biblio.ugent.be/publication/780271.json">
				</webAPI>
			</source>
		</logicalSource>
		<subjectMap>
			<template value="http://www.ex.com/paper/{_id}"/>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="PaperAuthor">
			<predicateMaps>
				<constant value="ex:author"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="PaperAuthor">
				<reference value="name"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
