<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="mail:" iri="http://example.com/mail#"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="ex:" iri="http://www.example.com/"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<directives prefix="rdf:" iri="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
	<directives prefix="transit:" iri="http://vocab.org/transit/terms/"/>
	<directives prefix="wgs84_pos:" iri="http://www.w3.org/2003/01/geo/wgs84_pos#"/>
	<directives prefix="schema:" iri="http://schema.org/"/>
	<directives prefix="gn:" iri="http://www.geonames.org/ontology#"/>
	<directives prefix="geosp:" iri="http://www.telegraphis.net/ontology/geography/geography#"/>
	<triplesMaps name="AirportMapping">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="src/test/resources/example3/Airport.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://airport.example.com/{id}"/>
			<class>transit:Stop</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="AirportMapping">
			<predicateMaps>
				<constant value="transit:service"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AirportMapping" parentTriplesMap="BusMapping">
				<joinConditions ownTriplesMap="AirportMapping" parent="@id" child="bus">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="AirportMapping">
			<predicateMaps>
				<constant value="wgs84_pos:location"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="AirportMapping" parentTriplesMap="VenueMapping">
				<joinConditions ownTriplesMap="AirportMapping" parent="$.location.city" child="city">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="BusMapping">
		<logicalSource  iterator="/transport/bus" referenceFormulation="ql:XPath">
			<source path="src/test/resources/example3/Transport.xml"/>
		</logicalSource>
		<subjectMap>
			<template value="http://trans.example.com/busses/{@id}"/>
			<class>transit:Service</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="BusMapping">
			<predicateMaps>
				<constant value="transit:stop"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="BusMapping" parentTriplesMap="BusStopMapping">
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="BusStopMapping">
		<logicalSource  iterator="/transport/bus/route/stop" referenceFormulation="ql:XPath">
			<source path="src/test/resources/example3/Transport.xml"/>
		</logicalSource>
		<subjectMap>
			<template value="http://trans.example.com/stops/{@id}"/>
			<class>transit:Stop</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="BusStopMapping">
			<predicateMaps>
				<constant value="rdfs:label"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="BusStopMapping">
				<reference value="."/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="VenueMapping">
		<logicalSource  iterator="$" referenceFormulation="ql:JSONPath">
			<source path="src/test/resources/example3/Venue.json"/>
		</logicalSource>
		<subjectMap>
			<template value="http://loc.example.com/city/{$.location.city}"/>
			<class>schema:City</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="VenueMapping">
			<predicateMaps>
				<constant value="wgs84_pos:location"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="VenueMapping" parentTriplesMap="LocationMapping">
				<joinConditions ownTriplesMap="VenueMapping" parent="$.location.city" child="$.location.city">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="VenueMapping">
			<predicateMaps>
				<constant value="geosp:onContinent"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="VenueMapping">
				<reference value="$.location.continent"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="VenueMapping">
			<predicateMaps>
				<constant value="gn:countryCode"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="VenueMapping">
				<reference value="$.location.country"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="LocationMapping">
		<logicalSource  iterator="$" referenceFormulation="ql:JSONPath">
			<source path="src/test/resources/example3/Venue.json"/>
		</logicalSource>
		<subjectMap>
			<template value="http://loc.example.com/latlong/{$.venue.latitude},{$.venue.longitude}"/>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="LocationMapping">
			<predicateMaps>
				<constant value="wgs84_pos:lat"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="LocationMapping">
				<reference value="$.venue.latitude"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="LocationMapping">
			<predicateMaps>
				<constant value="wgs84_pos:long"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="LocationMapping">
				<reference value="$.venue.longitude"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
