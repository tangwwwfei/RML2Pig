<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="ex:" iri="http://www.example.com/"/>
	<triplesMaps name="Author">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="authors.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://example.com/author/{ID}"/>
			<class>ex:Person</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="Author">
			<predicateMaps>
				<constant value="ex:name"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Author">
				<reference value="name"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Author">
			<predicateMaps>
				<constant value="ex:position"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Author">
				<reference value="position"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Author">
			<predicateMaps>
				<constant value="ex:worksForDepartment"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Author" parentTriplesMap="Department">
				<joinConditions ownTriplesMap="Author" parent="ID" child="uni">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="Paper">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="papers.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://example.com/paper/{ID}"/>
			<class>ex:Paper</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="Paper">
			<predicateMaps>
				<constant value="ex:title"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Paper">
				<reference value="title"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Paper">
			<predicateMaps>
				<constant value="ex:year"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Paper">
				<reference value="year"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Paper">
			<predicateMaps>
				<constant value="ex:conference"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Paper">
				<reference value="conference"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Paper">
			<predicateMaps>
				<constant value="ex:author"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Paper" parentTriplesMap="Author">
				<joinConditions ownTriplesMap="Paper" parent="ID" child="author">
				</joinConditions>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
	<triplesMaps name="Department">
		<logicalSource  referenceFormulation="ql:CSV">
			<source path="universities.csv"/>
		</logicalSource>
		<subjectMap>
			<template value="http://example.com/department/{ID}"/>
			<class>ex:Department</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="Department">
			<predicateMaps>
				<constant value="ex:name"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Department">
				<reference value="department"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Department">
			<predicateMaps>
				<constant value="ex:country"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Department">
				<reference value="country"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="Department">
			<predicateMaps>
				<constant value="ex:partOf"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="Department">
				<reference value="name"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
