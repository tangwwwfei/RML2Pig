<?xml version="1.0" encoding="ASCII"?>
<rml:Mapping xmi:version="2.0" 
	xmlns:xmi="http://www.omg.org/XMI" 
	xmlns:rml="http://www.xtext.org/example/rml/RML">
	<directives prefix="rr:" iri="http://www.w3.org/ns/r2rml#"/>
	<directives prefix="rml:" iri="http://semweb.mmlab.be/ns/rml#"/>
	<directives prefix="ex:" iri="http://semweb.mmlab.be/rml/examples/spc/ns#"/>
	<directives prefix="ql:" iri="http://semweb.mmlab.be/ns/ql#"/>
	<directives prefix="xsd:" iri="http://www.w3.org/2001/XMLSchema#"/>
	<directives prefix="rdfs:" iri="http://www.w3.org/2000/01/rdf-schema#"/>
	<directives prefix="dcterms:" iri="http://purl.org/dc/terms/"/>
	<directives prefix="tio:" iri="http://purl.org/tio/ns#"/>
	<directives prefix="bibo:" iri="http://purl.org/ontology/bibo/"/>
	<directives prefix="skos:" iri="http://www.w3.org/2004/02/skos/core#"/>
	<directives prefix="opus:" iri="http://lsdis.cs.uga.edu/projects/semdis/opus#"/>
	<directives prefix="foaf:" iri="http://xmlns.com/foaf/"/>
	<directives prefix="swc:" iri="http://data.semanticweb.org/ns/swc/ontology#"/>
	<directives prefix="spc:" iri="http://semweb.mmlab.be/ns/spc/"/>
	<directives prefix="mads:" iri="http://www.loc.gov/mads/rdf/v1#"/>
	<directives prefix="opus:" iri="http://lsdis.cs.uga.edu/projects/semdis/opus#"/>
	<directives prefix="hydra:" iri="http://www.w3.org/ns/hydra/core#"/>
	<triplesMaps name="VolumeMapping">
		<logicalSource  iterator="body" referenceFormulation="ql:CSS3">
			<source>
				<webAPI template="http://ceur-ws.org/Vol-{number}/">
					<hydraMapping variable="number" required="true">
					</hydraMapping>
				</webAPI>
			</source>
		</logicalSource>
		<subjectMap>
			<template value="http://ceur-ws.org/{span.CEURVOLNR}/"/>
			<class>bibo:Volume</class>
		</subjectMap>
		<predicateObjectMaps ownTriplesMap="VolumeMapping">
			<predicateMaps>
				<constant value="bibo:volume"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="VolumeMapping">
				<reference value="span.CEURVOLNR"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="VolumeMapping">
			<predicateMaps>
				<constant value="dcterms:identifier"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="VolumeMapping">
				<reference value="span.CEURURN"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="VolumeMapping">
			<predicateMaps>
				<constant value="bibo:shortTitle"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="VolumeMapping">
				<reference value="span.CEURVOLACRONYM"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="VolumeMapping">
			<predicateMaps>
				<constant value="rdfs:label"/>
			</predicateMaps>
			<predicateMaps>
				<constant value="spc:volTitle"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="VolumeMapping">
				<reference value="span.CEURVOLTITLE a"/>
			</objectMaps>
		</predicateObjectMaps>
		<predicateObjectMaps ownTriplesMap="VolumeMapping">
			<predicateMaps>
				<constant value="rdfs:label"/>
			</predicateMaps>
			<predicateMaps>
				<constant value="spc:fullTitle"/>
			</predicateMaps>
			<objectMaps ownTriplesMap="VolumeMapping">
				<reference value="span.CEURFULLTITLE"/>
			</objectMaps>
		</predicateObjectMaps>
	</triplesMaps>
</rml:Mapping>
